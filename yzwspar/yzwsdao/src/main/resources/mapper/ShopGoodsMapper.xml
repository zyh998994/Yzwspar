<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.dao.ShopGoodsDao">
    <select id="shopAll" resultType="ShopGoods">
        select goodsname,goodspice,goodssales,goodsimg from shopgoods where shortid=#{shortid}
    </select>
</mapper>
